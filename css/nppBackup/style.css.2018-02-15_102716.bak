@import url(reset.css);
body{
	font: 12px Arial, Tahoma, Verdana, sans-serif;
	color:#5d5d5d;
}
a{
	color:#fe9b00;
	text-decoration:none;
	font-weight:bold;
	text-transform:uppercase;
}
#topLine{
	background:#090909;
	height:16px;
}
#wrapper{
	outline:1px solid #000000;
	width:960px;
	margin:0 auto;	
}
.header{
	padding:20px 0 17px 0;
	border-bottom:1px solid #c0c0c0;
	overflow: hidden;
}
.aHome, a:hover{
	color:#090909;
}

.logo{
	width:183px;
	float:left;
}
.menu{
	float:right;
	margin-bottom:45px;
}
.menu ul li{
	height:100px;
	float:left;
	margin-left:63px;
	margin-top:17px;
}