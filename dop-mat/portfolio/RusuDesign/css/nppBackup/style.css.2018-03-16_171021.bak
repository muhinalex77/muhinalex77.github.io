@import url(reset.css);
@import url('https://fonts.googleapis.com/css?family=Source+Sans+Pro:200,200i,300,300i,400,400i,600,600i,700,700i,900,900i');

body{
	font-family: 'Source Sans Pro', sans-serif;
	background:#e6e6e6;
}
.wrapper{
	min-width:960px;
	max-width:2560px;
	margin:0 auto;
	width:100%;
	
}
a{
	text-decoration:none;
	color:#777777;
}
h1{
	font-size:40px;
	font-weight:900;
	color:#f5f5f5;
}
h3{
	font-size:26px;
	margin-bottom:6px;
}
p{
	font-size:18px;
	font-weight:300;
}
span{
color:#ed9e2d;
}
.spanBg{
	background-color:rgba(12,31,44,0.5);
	margin-left:9px;
}
.size {
	outline:1px solid #000;
	width:960px;
	margin:0 auto;
}
.header{
	padding:27px 0;
}
.inner-header-content{
	display:flex;
	
}
.inner-header-content,
.inner-headerText{
	margin:0 10px;
}
.logo,.middleText,.headPhone{
	width:33.3%;
}
.logo{
	margin-top:9px;
}
.middleText{
	text-align:center;	
}
.middleText h3{
	font-weight:900;
	text-transform:uppercase;
}
.headPhone{
	text-align:right;
}
.headPhone h3{
	font-weight:bold;
}
.first-content-wrapper{
	background:url(../images/bgContent.jpg) no-repeat;
}
