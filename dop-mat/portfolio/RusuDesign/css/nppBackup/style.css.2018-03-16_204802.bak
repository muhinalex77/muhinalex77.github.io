@import url(reset.css);
@import url('https://fonts.googleapis.com/css?family=Source+Sans+Pro:200,200i,300,300i,400,400i,600,600i,700,700i,900,900i');

body{
	font-family: 'Source Sans Pro', sans-serif;
	background:#e6e6e6;
}

.clear:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
  clear: both;
  height: 0;
}
.left{
	float:left;
}
.right{
	float:right
}
.clear { display: inline-block; }
/* start commented backslash hack \*/
* html .clear { height: 1%; }
.clear { display: block; }
/* close commented backslash hack */

.left{
	float:left;
}
.right{
	float:right;
}

.wrapper{
	min-width:960px;
	max-width:2560px;
	margin:0 auto;
	width:100%;
}
a{
	text-decoration:none;
	color:#777777;
}
h1{
	font-size:40pt;
	font-weight:900;
	color:#f5f5f5;
	line-height:1.3;
}
h3{
	font-size:26px;
	margin-bottom:6px;
}
h6{
	font-size:16pt;
	color:#f5f5f5;
}
p{
	font-size:18px;
	font-weight:300;
}
span{
color:#ed9e2d;
}
.spanBg{
	background-color:rgba(12,31,44,0.5);
	margin-left:9px;
}
.size {
	outline:1px solid #000;
	width:960px;
	margin:0 auto;
}
.header{
	padding:27px 0;
}
.inner-header-content{
	display:flex;
	
}
.inner-header-content,
.inner-first-content{
	margin:0 10px;
}
.logo,.middleText,.headPhone{
	width:33.3%;
}
.logo{
	margin-top:9px;
}
.middleText{
	text-align:center;	
}
.middleText h3{
	font-weight:900;
	text-transform:uppercase;
}
.headPhone{
	text-align:right;
}
.headPhone h3{
	font-weight:bold;
}
.first-content-wrapper{
	background:url(../images/bgContent.jpg) no-repeat;
}
.inner-first-content-wrapper{
	padding-top: 37px;
}
.timerBox{
	width:480px;
}
.timerBox-text{
	width:160px;
	font-weight:700;
}
.timer{
	width:320;
}
.formBox{
	width:460px;
}