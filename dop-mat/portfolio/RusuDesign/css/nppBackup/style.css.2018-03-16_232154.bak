@import url(reset.css);
@import url('https://fonts.googleapis.com/css?family=Source+Sans+Pro:200,200i,300,300i,400,400i,600,600i,700,700i,900,900i');

body{
	font-family: 'Source Sans Pro', sans-serif;
	background:#e6e6e6;
	
}

.clear:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
  clear: both;
  height: 0;
}
.left{
	float:left;
}
.right{
	float:right
}
.clear { display: inline-block; }
/* start commented backslash hack \*/
* html .clear { height: 1%; }
.clear { display: block; }
/* close commented backslash hack */

.left{
	float:left;
}
.right{
	float:right;
}

.wrapper{
	min-width:960px;
	max-width:2560px;
	margin:0 auto;
	width:100%;
}
a{
	text-decoration:none;
	color:#777777;
}
h1{
	font-size:40px;
	font-weight:900;
	color:#f5f5f5;
	line-height:1.3;
}
h2{
	font-size:30px;
}
h3{
	font-size:26px;
	margin-bottom:6px;
}
h4{
	font-size:22px;
}
h6{
	font-size:16px;
	color:#f5f5f5;
}
p{
	font-size:18px;
	font-weight:300;
}
span{
color:#ed9e2d;
}
.spanBg{
	background-color:rgba(12,31,44,0.5);
	margin-left:9px;
}
.size {
	outline:1px solid #000;
	width:960px;
	margin:0 auto;
}
.header{
	padding:27px 0;
}
.inner-header-content{
	display:flex;
	
}
.inner-header-content,
.inner-first-content{
	margin:0 10px;
}
.logo,.middleText,.headPhone{
	width:33.3%;
}
.logo{
	margin-top:9px;
}
.middleText{
	text-align:center;	
}
.middleText h3{
	font-weight:900;
	text-transform:uppercase;
}
.headPhone{
	text-align:right;
}
.headPhone h3{
	font-weight:bold;
}
.first-content-wrapper{
	background:url(../images/bgContent.jpg) no-repeat;
}
.inner-first-content-wrapper{
	padding-top: 37px;
}
.timerBox{
	width:480px;
}
.timerBox-text{
	width:160px;
}
.inner-timerBox-text{
		padding:9px 0 0 2px;;
}
.timerBox-text h6{
	font-weight:700;
	text-transform:uppercase;
	line-height:1.2;
	letter-spacing:0.5px;	
}
.inner-timerBox{
	margin-top:103px;	
}
.timer{
	width:320;
}
.formBox{
	width:460px;
}
.form{
	background:rgba(48,65,78,0.8);
	width:430px;
	border-top:3px solid #ED9E2D;
	box-shadow: 0 0 10px rgba(12,31,44,0.7);
}
.inner-formBox{
	margin-top:31px;
}

form h4{
	font-weight:500;
	text-align:center;
	margin-bottom:15px;
}
form h2{
	font-weight:900;
	margin-left:30px;
	text-align:justify;
	margin-bottom:15px;
}
input{
	color:#B9BEBA;
	border:none;
	margin:15px 30px 0 30px;
	width:320px;
	height:38px;
	padding-left:44px;
	background:url(../images/personIcon.png) no-repeat, #fff;
}
input[name="phone"]{
	background:url(../images/phoneIcon.png) no-repeat, #fff;
}
input[name="email"]{
	background:url(../images/mailIcon.png) no-repeat, #fff;
}
input[type="submit"]{
	font-size:19px;
	text-transform:uppercase;
	padding-right:30px;
	margin:30px 0 30px 135px;
	border:none;
	width:260px;
	height:54px;
	background:#cc144b;
	box-shadow: 0 0 5px rgba(12,31,44,0.5);
}
input[type="submit"]:hover{
	background:#860d31;
	
}
