    top: 92px;
    left: 129px;
}
.line2{
    position: relative;
    top: 64px;
    left: 251px;
}
.line3{
    position: relative;
    top: 67px;
    left: 468px;
}

.line4{
    position: relative;
    top: 56px;
    left: 663px;
}
.third-content-blocks{
	position:relative;
}
.portfolioContent{
	width:25%;
}
.portfolioButton{
	margin-top:133px;
}
.portfolioButton button{
	font-size: 15px;
    padding: 16px 31px 16px 32px;
    letter-spacing: 0px;
}