@import url(reset.css);
@import url('https://fonts.googleapis.com/css?family=Open+Sans');
body{
	font-family:'Open Sans', sans-serif;'
	background:#1b1817;
	text-align:center;
	line-height:1.5;
}
.clear:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
  clear: both;
  height: 0;
}
.clear { display: inline-block; }
/* start commented backslash hack \*/
* html .clear { height: 1%; }
.clear { display: block; }
/* close commented backslash hack */
h1{
	font-size:49px;
	font-weight:bold;
	color:#ffffff;
	text-transform:uppercase;
}
h2{
	font-size:39px;
	color:#ffffff;
	font-weight:semi-bold;
	margin:35px 0;
}
h3{
	font-size:27px;
	color:#000;
	font-weight:bold;
	padding:10px;
	text-align:left;
}
p{
	font-size:14px;
	color:#938e8e;
}
.wrapper{
	width:1280px;
	margin:0 auto;
}
.header{
	background:url(../images/bgH.jpg) no-repeat;
	height:800px;
}
.logo img{
	border-radius:50%;
	margin:70px;
	margin-bottom:50px;
	position:relative;
	left:-2px;
}
.headerForm{
	width:334px;
	background:#fff;
	border-radius:3px;
	position:relative;
	left:37%;
	bottom:-10px;
	border:1px solid #ebebeb;
}
input[type="text"]{
	margin:30px 30px 25px 30px;
	font-size:16px;
	color:#e5e5e5;
	padding:12px;
	border:1px solid #e5e5e5;
	border-top-width:2px;
	border-radius:2px;
	outline:none;
}
.headerForm img, p{
	margin-bottom:30px;
}
input[type="submit"]{
	background:#227dd3;
	border-radius:2px;
	border:none;
	outline:none;
	color:#fff;
	font-size:25px;
	padding:13px 35px;
	margin-bottom:30px;
}
.upperContent{
	padding-top:180px;
	padding-bottom:100px;
}
.contBlock{
	float:left;
	width:50%;	
}
.contBlock p{
	text-align:left;
	
}
.block1,.block2{
	width:420px;
}
.block1{
	margin-left:215px;
}
.block2{
	margin-left:35px;
}
.contBlock img{
	float:left;
	margin-right:30px;
}
.bottomContent{
	background:url(../images/bgContent.png) no-repeat;
	height:1352px;
}
.botContent_1{
	padding:80px 300px;
}
.botContent_1 p, .botContent_2 p{
	color:#c8e7d5;
	text-align:left;
	margin-bottom:30px;
	line-height:1.5;
}

/*#img{

-webkit-filter: drop-shadow(-50px 50px 1px #46a671);
filter: drop-shadow(-300px 180px 1px #46a671);
}*/
.botContent_2{
	margin:240px 200px 240px 250px;
}
.botContent_2 img{
	float:left;
	margin-right:80px;
	margin-bottom:50px;
	
}
.botContent_2 h2{
	text-align:left;
	margin-bottom:10px;
}
button{
	position:relative;
	left:-140px;
	padding:14px 35px;
	background:none;
	border:3px solid #fff;
	border-radius:5px;
	font-size:28px;
	color:#fff;
	text-align:left;
}
.inner-footer{
	height:635px;
	background:url(../images/bgMap.jpg) no-repeat;
	padding-top:100px;
}
.footerForm{
	margin:0 auto;
	width:430px;
	height:430px;
	border-radius:50%;
	background:rgba( 255,255,255, 0.7)
}

.form{
	padding-top:90px;
}
.form input[type="text"]{
	width:275px;
	margin:0;
}
