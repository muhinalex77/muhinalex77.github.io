@import url(reset.css);
*{
	box-sizing:border-box;
}
body{
	color:#ffffff;
	font-size:30px;
}
.clear:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
  clear: both;
  height: 0;
}
.clear { display: inline-block; }
/* start commented backslash hack \*/
* html .clearfix { height: 1%; }
.clear { display: block; }
/* close commented backslash hack */
