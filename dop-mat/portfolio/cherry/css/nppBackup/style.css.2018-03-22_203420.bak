@import url(reset.css);
body{
	font-family:Arial, sans-serif;
}
.wrapper{
	max-width:1566px;
	min-width:980px;
	margin:0 auto;
	width:100%;
	background:url(../images/bodyBg.png);
}
.clear:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
  clear: both;
  height: 0;
}
.left{
	float:left;
}
.right{
	float:right
}
.clear { display: inline-block; }
/* start commented backslash hack \*/
* html .clear { height: 1%; }
.clear { display: block; }
/* close commented backslash hack */

h1{
	font-size:30px;
	font-weight:bold;

}
h2{
	font-size:24px;
}
h3{
	font-size:18px;
}
h4{
	font-size:16px;
}
h5{
	font-size:14px;
	font-weight:bold;
}
h6{
	font-size:12px;
	font-weight:bold;
	color:#c656f7a;
}
p{
	font-size:14px;
	color:#d9dcdf;
}
a{
	text-decoration:none;
	color:inherit;
}
.header{
	background:url(../images/footerBg.png)repeat-x;
}
.header-content,
.top-content,
.nav,
.nav-tree,
.main-content-wrapper
{
	outline:1px solid #fff;
	width:960px;
	margin:0 auto;
	padding:0 10px;
} 
.inner-header-content{
	margin-right:-6px;
}
.logo{
	margin:18px 0 15px;
	line-height:0.95;
	}
.logo h1{
	color:#a5adb8;
	text-transform:uppercase;
}
.logo a {
	
}
.header h6{
	color:#656f7a;
}
.red-letter{
	color:#ae4b5d;
}
.search input{
	float:left;
}
.search input[type="text"]{
	border:1px solid #875f66;
	border-radius:4px;
	padding-left:9px;
	letter-spacing:0.5px;
	height:29px;
	width:187px;
	color:#e5b636e;
	margin-right:12px;
	background:#24272d;
}
.search{
	margin:20px 0 18px;
}
.top-content{
	background:url(../images/bg.jpg) no-repeat;
}
.inner-top-header-content{
	padding: 41px 29px 37px;
}
.top-header-content h1{
	color:#a5adb8;
}
.blackFont{
	color:#282d33;
    letter-spacing: 0.6px;
}
.first-H3{
	margin-bottom:18px;
}
.second-H3{
	margin-bottom:6px;
	letter-spacing: 0.6px;
}
.top-header-content h3{
	color:#a5adb8;
	padding-left: 2px;
}
.top-header-content p{
    line-height: 1.3;
	letter-spacing: 0.1px;
    padding-left: 1px;
}
.nav{
	background:#95161f;
}
.inner-nav{
	padding:10px;
}
.nav ul{
	float:right;
}
.nav ul li{
	display:inline-block;
	float:left;
	font-size:16px;
	font-weight:bold;
	color:#fff;
	margin:0 26px;
}
#contact{
	margin:0 0 0 25px;
}
.nav-tree{
	background:#f3f3f3;
}
.inner-nav-tree{
	padding: 7px 14px;
}
.nav-tree p{
	font-size:11px;
	color:#6b6f74;
}
.nav-tree a{
	text-decoration:underline;
	color:#95161f;
}
.main-content-wrapper{
	background:#fff;
}
.sidebar{
	width:26.325%
}
.inner-sidebar{
	padding: 21px 26px 0 12px;
}
.main-content{
	width:73.675%
}
.inner-sidebar-form{
	padding:15px 0 31px 10px;
}
.sidebar-form{
	background:#f3f3f3;
	border:1px solid #d8dbde;
}
.sidebar-form h3{
	font-weight:bold;
	padding-left:2px;
	margin-bottom:15px;
}
.sidebar input[type="text"]{
	font-size:14px;
	text-transform:capitalize;
	color:#757575;
	border:1px solid #fff;
	border-radius:3px;
	background:#e3e4e5;
	padding-left:10px;
	width:174px;
	height:31px;
	margin-bottom:12px;
}
.sidebar input[name="email"]{
	margin-bottom:9px;
} 
.sidebar input[type="submit"]{
	background:#a21d27;
	border:4px solid #fff;
	font-size:16px;
	color:#fff;
	margin:2px;
    padding: 10px 18px;
	box-shadow:0 0 10px rgba(0,0,0,0.5);
}
.inner-main-content{
	padding-top:19px;
}
.photo-col{
	display:inline-block;
	float:left;
}
.main-content h2{
	color:#a5adb8;
	font-weight:bold;
	text-transform: capitalize;
	margin-bottom:11px;
}
.main-content p{
	color:#5c646e;
	margin-bottom:14px;
	line-height:1.45;
	letter-spacing:0.1px;
}
.first-content a{
	color:#95161f;
	text-decoration:underline;
}
.photo-col{
	margin-right:11px;
}
.photo-col h3{
	font-weight:bold;
	color:#2f343a;
	margin-bottom:12px;
}
.photo-col img{
	border:2px solid #fff;
	box-shadow: 0 5px 10px rgba(0,0,0,0.5);
}
.second-content{
	margin-bottom:27px;
}
.inner-second-content{
	margin-top:25px;
}
.list{
	width:33.333%;
	display:inline-block;
	float:left;
}
.inner-list{
	margin-top:6px;
	
}
.list h5{
	margin-bottom:8px;
	color:#6c737b;
}
.list h5 span{
	background:#dedede;
	color:#5c646e;
}
.list-ol ol,
.list ul{
	color:#5c646e;
	padding-left: 22px;
    line-height: 1.1;
}

.list-ol ol li,
.list ul li{
	font-size: 14px;
    list-style-type: disc;
    margin-bottom: 7px;
}
.list ul li ul{
	padding:0;
	line-height:1.2;
	margin-top:8px;
	margin-left:18px;
}
.inner-unstyled{
	margin-left:7px;
}
.unstyled ul{
	padding:0;
}
.unstyled ul li{
	list-style-type:none;	
}
.unstyled ul li ul{
	padding-left:4px;
	
}
.unstyled ul li ul li{
	list-style-type:disc;
}
.list-ol h5{
	margin-bottom:6px;
}
.list-ol ol{
	padding-left: 16px;
    line-height: 16px;
}
.list-ol ol li{
	list-style-type:decimal;
}
.table h2{
	margin-bottom:19px;
}
table{
	width:100%;
	border:1px solid #fff;
	font-size:12px;
	margin-left:1px;
	border-collapse: collapse;
}
th{
	border: 1px solid black;
	font-weight:bold;
	background:#9c4757;
	color:#fff;
	text-align:center;
	padding:10px 0;
}
#first-th{
	border-radius:10px 0 0 0;
}
#last-th{
	border-radius:0 10px 0 0;
}

td{
	border: 1px solid black;
	background:#282d33;
	padding:10 0px;
	color:#a5adb8;
	font-size:12px;
	padding:10px 13px;
}