@import url(reset.css);

.clear{
	clear:both;
	font:13px/1.4 Arial, Verdana, Tahoma, sans-serif;
}
body{
	background-image:url(../images/Bg.png);
}
a{
	text-decoration:none;
}
#wrapper{
	min-width:960px;
	max-width:1200px;
	margin:0 auto;
	margin-top:2px;
	}
#header{
	outline:1px solid #090909;
	margin:0 auto;	
	overflow:hidden;
}
#logo{
	float:left;
	width:241px;
	height:46px;
	margin:44px 0 42px 10px;
	
	}
#baner{
	float:right;
	width:468px;
	height:60px;
	margin:34px 10px 32px 0;
}
#topNavMenu{
	background:url(../images/menuBg.png);
	height:49px;
	width:100%;
}
#innerNavMenu ul li{
	float:left;
	margin:19px 4px 19px 16px;
}
#innerNavMenu a{
	text-decoration:none;
	text-transform:uppercase;
	font:12px Arial, Verdana, Tahoma, sans-serif;
	color:#ebebeb;
}