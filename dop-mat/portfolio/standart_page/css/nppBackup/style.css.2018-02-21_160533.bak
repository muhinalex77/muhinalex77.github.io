@import url(reset.css);

body{
	font:20px/1 Arial, Tahoma, Verdana, sans-serif;
	color:#333333;
}
#wrapper{
	width:1200px;
	margin:0 auto;
	max-width:1200px;
	min-width:960px;
}
#header{
	
	background:url(../images/headerBg.png);
	
}
.clear{
	clear:both;
}
.left{
	width:50%;
	float:left;
	display:block;
}
.right{
	width:50%;
	float:right;
	
}
h1{
	font: 60px Arial, Tahoma, Verdana, sans-serif;
	color:#ffffff;
	margin-bottom:30px;
	}
.headerContent{
	padding:40px
}
.headerContent  ul li{
	color:#ffffff;
	margin-top:15px;
}
.headerContent ul li{
	list-style:url(../images/chek.png);
	list-style-position:inside;
}
