@import url(reset.css);

body{
	font:16px/1.3 Arial, Tahoma, Verdana, sans-serif;
	color:#333333;
}
#wrapper{
	width:1200px;
	margin:0 auto;
	max-width:1200px;
	min-width:960px;
}
#header{
	
	background:url(../images/headerBg.png);
	
}
.clear{
	clear:both;
}
.left{
	width:50%;
	float:left;
	display:block;
}
.right{
	
	float:right;
	display:block;
}
h1{
	font: 60px Arial, Tahoma, Verdana, sans-serif;
	color:#ffffff;
	margin-bottom:30px;
}
h2{
	font:30px Arial, Tahoma, Verdana, sans-serif;
	padding-bottom:14px;
	color:#445161;
}
.headerContent, .inner{
	padding:50px
}
.headerContent  ul li{
	color:#ffffff;
	margin-top:15px;
	font-size:20px;
}
.headerContent ul li{
	list-style:url(../images/chek.png);
	list-style-position:inside;
}
#information{
	
	background:#ffffff;
}
#contentAbout p{
	margin-top:20px;
}
.productP{
	width:443px;
	height:245px;
	background:#445161;
}
#advantage{
	background:#f5f5f5;
}
.headers{
	text-align:center;
}
.width{
 	width:50%;
}
#advContent ul li{
	background:url(../images/plus.png) no-repeat 0 20px;
	padding-left:35px;
	padding-top:20px;
}
#contentScreen{
	overflow:hidden;
}
.ScrShCont{
	overflow:hidden;
	margin-bottom:5px;
}
.productS{
	background:#445161;
	width:200px;
	height:150px;
}